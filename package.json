{
  "name": "@moyuyc/parse-git-config",
  "description": "Parse `.git/config` into a JavaScript object. sync or async.",
  "version": "2.0.1",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Sam Holmes (https://samholmes.net)",
    " (https://github.com/js-n)"
  ],
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "expand-tilde": "^2.0.2",
    "git-config-path": "^1.0.1",
    "ini": "^1.3.5",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "homedir-polyfill": "^1.0.1",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "config",
    "git",
    "parse"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "git-user-name",
        "git-username",
        "parse-author",
        "parse-authors",
        "parse-github-url",
        "parse-gitignore"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imcuttle/parse-git-config.git"
  },
  "bugs": {
    "url": "https://github.com/imcuttle/parse-git-config/issues"
  },
  "homepage": "https://github.com/imcuttle/parse-git-config#readme"
}
